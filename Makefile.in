.POSIX:
.SUFFIXES:

TARGET = noded
RM ?= rm -f

CC = @@CC@@
CFLAGS = @@CFLAGS@@ -Iinclude
LDFLAGS = @@LDFLAGS@@
INCLUDES = @@INCLUDES@@

default: noded
all: noded tools

tools: $(TOOLS)

include build.make

clean:
	$(RM) $(TARGET) $(TOOLS) $(TESTS) $(OBJECTS) $(OTHER_OBJS)

.PHONY: default all tools test clean
